{
  "name": "ts-has-property",
  "version": "2.1.2",
  "description": "Universal and better typed `hasOwnProperty` for IntelliSense. Supports checking: properties of all types; multiple keys at a time; and type of values.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Said Magomedov <saids.inbox@gmail.com>",
  "repository": "https://github.com/said-m/ts-has-property",
  "license": "MIT",
  "keywords": [
    "type",
    "hasOwnProperty",
    "TypeScript",
    "Object",
    "check",
    "property",
    "util",
    "editor",
    "hint"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jest.config.json",
    "test:coverage": "yarn test --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepack": "yarn build",
    "postpack": "rm -rf ./dist",
    "prepare": "husky install"
  },
  "dependencies": {
    "@said-m/common": "1.0.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "coveralls": "^3.1.1",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=12"
  }
}
